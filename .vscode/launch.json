{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run DayZ Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/vscode/out/**/*.js"
      ],
      "preLaunchTask": "watch"
    }
  ]
}